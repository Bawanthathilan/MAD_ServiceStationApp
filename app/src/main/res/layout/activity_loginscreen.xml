<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/logbag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logh"
    tools:context=".Loginscreen">

    <EditText
        android:id="@+id/logtext"
        android:layout_width="330dp"
        android:layout_height="55dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="387dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="299dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="enter email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/logpass"
        android:layout_width="329dp"
        android:layout_height="55dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="225dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="enter email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logtext" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="274dp"
        android:layout_height="257dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="86dp"
        app:layout_constraintBottom_toTopOf="@+id/logtext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/jh" />

    <Button
        android:id="@+id/logbut"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="41dp"
        android:background="@drawable/a"
        android:onClick="onButtonClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logpass" />

</android.support.constraint.ConstraintLayout>